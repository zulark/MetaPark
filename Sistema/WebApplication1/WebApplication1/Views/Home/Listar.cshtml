@using WebApplication1
@using WebApplication1.Models
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@model List<Usuario>;

<!DOCTYPE html>
<html>
<head>
    <title>Inicio</title>
</head>
<body>

    @for (int i = 0; i < @Model.Count; i++)
    {
        <h1>Nomes</h1>
        @Model[i].Nome
    }

    <p>O número total de nomes cadastrados é: @Model.Count</p>

    <a asp-action="Login" text-decoration:none>Voltar</a>
</body>
</html>